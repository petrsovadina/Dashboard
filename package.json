{
  "name": "azure-ai-dashboard",
  "version": "1.0.0",
  "description": "Kompletní real-time dashboard pro monitoring cen tokenů a usage Azure AI modelů s podporou CZK konverze, autentifikace a WebSocket aktualizací.",
  "main": "azure_realtime_backend.js",
  "scripts": {
    "start": "node azure_realtime_backend.js",
    "dev": "nodemon azure_realtime_backend.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f azure-ai-dashboard",
    "serena": "uvx --from git+https://github.com/oraios/serena serena start-mcp-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petrsovadina/Dashboard.git"
  },
  "keywords": [
    "azure",
    "ai",
    "dashboard",
    "monitoring",
    "real-time",
    "typescript",
    "react",
    "websocket"
  ],
  "author": "Petr Sovadina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/petrsovadina/Dashboard/issues"
  },
  "homepage": "https://github.com/petrsovadina/Dashboard#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ws": "^8.18.0",
    "node-cron": "^3.0.3",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
